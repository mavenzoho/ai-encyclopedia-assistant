# Google API Key (get from https://aistudio.google.com/apikey)
GOOGLE_API_KEY=your-gemini-api-key-here

# Google Cloud Project (for Vertex AI / Cloud Run deployment)
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
GOOGLE_CLOUD_LOCATION=us-central1

# Set to TRUE to use Vertex AI instead of Google AI Studio
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# Models (defaults work out of the box)
LIVE_API_MODEL=gemini-2.5-flash
IMAGE_GEN_MODEL=gemini-2.5-flash-image

# Server port (Cloud Run sets this automatically)
PORT=8080
